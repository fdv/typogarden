<h3 id="syndicate-heading">Syndicate</h3>
<ul id="syndicate-list">
  <% if @sb_config['articles'] -%>
  <li class="RSS"><a href="<%= url_for :controller=>'xml', :action=>'rss' %>" title="RSS 2.0 feed">Articles</a></li>
  <% end %>
  <% if @sb_config['comments'] -%>
  <li class="RSS-Comments"><a href="<%= url_for :controller=>'xml', :action=>'commentrss' %>" title="Comments RSS 2.0 feed">Comments</a></li>
  <% end %>
  <% if @sb_config['trackbacks'] -%>
  <li class="trackbacks"><a href="<%= url_for :controller=>'xml', :action=>'trackbackrss' %>" title="Trackbacks RSS 2.0 feed">Trackbacks</a></li>
  <% end %>
</ul>
