<% if @fortythreeplaces -%>
<h3 id="forty-three-places-heading"><%= link_to "43 Places", @fortythreeplaces.link %></h3>
<ul id="forty-three-places-list">
<% for thing in @fortythreeplaces.things.to_a.slice(0,@sb_config['count'].to_i) -%>
  <li><a href="<%= thing.link %>" title="<%= thing.title %>"><%= thing.title %> </a></li>
<% end -%>
</ul>
<% end -%>
