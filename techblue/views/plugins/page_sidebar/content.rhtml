<% unless sidebar.pages.blank? -%>
<div class="sidemenu-head">
  <h3><%= _("Pages")%></h3>
</div>
<div class="sidemenu-content">
    <ul>
        <% for page in sidebar.pages -%>
            <li>
				<%= link_to page.title, page.permalink_url %>
            </li>
        <% end %>
    </ul>
</div>
<div class="sidemen-foot"></div>
<% end %>